deb_host_multiarch=
Name: stlink
Description: Open source version of the STMicroelectronics Stlink Tools
Version: 1.5.1
Requires: libusb-1.0
prefix=/usr/local
includedir=${prefix}/include/${deb_host_multiarch}/stlink
libdir=${prefix}/lib/${deb_host_multiarch}
Libs: -L${libdir} -l:libstlink.so.1
Cflags: -I${includedir}

